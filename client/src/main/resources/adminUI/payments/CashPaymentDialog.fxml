<AnchorPane fx:id="dialog" prefHeight="145.0" prefWidth="257.0" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.controller.sale.payments.CashPaymentReceivedController">
   <Text layoutX="14.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cash Payment">
      <font>
         <Font size="15.0"/>
      </font>
   </Text>
   <Label fx:id="totalLabel" layoutX="42.0" layoutY="37.0" prefHeight="17.0" prefWidth="155.0"/>
   <Label layoutX="12.0" layoutY="58.0" text="Amount received: "/>
   <TextField fx:id="amountReceivedField" layoutX="112.0" layoutY="54.0" prefHeight="25.0" prefWidth="85.0"/>
   <ButtonBar layoutX="12.0" layoutY="97.0" prefHeight="40.0" prefWidth="200.0">
      <buttons>
         <Button fx:id="confirmButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="139.0" text="Confirm"/>
      </buttons>
   </ButtonBar>
   <Label layoutX="12.0" layoutY="37.0" text="Total:"/>
   <Label fx:id="errorLabel" layoutX="9.0" layoutY="89.0" prefHeight="17.0" prefWidth="240.0" textAlignment="CENTER"
          textFill="RED"/>
</AnchorPane>
