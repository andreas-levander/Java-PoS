<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="savedSalesVBox" alignment="CENTER" prefHeight="255.0" prefWidth="323.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controller.cart.SavedCartsDialogController">

    <Label prefHeight="17.0" prefWidth="71.0" text="Saved sales">
      <font>
         <Font size="13.0" />
      </font></Label>
   <ListView fx:id="savedSalesListView" prefHeight="200.0" prefWidth="200.0" />
   <ButtonBar maxWidth="-Infinity" prefHeight="37.0" prefWidth="196.0">
     <buttons>
          <Button fx:id="getSale" mnemonicParsing="false" text="get cart" translateX="-15.0" />
          <Button fx:id="removeSale" minWidth="0.0" mnemonicParsing="false" prefHeight="25.0" text="remove cart" />
     </buttons>
   </ButtonBar>

</VBox>
